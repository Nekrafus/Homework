import Cocoa

var array = [2]

    var n = 3

var prime = true
while array.count <= 100
    


{
    
   for i in array
   {
    if n % i == 0
    
    {
    prime = false
    break
    }
    
    }
    if prime == true {
    array.append(n)
    }
    else
    {
    prime = true
    }
    n = n+1
}

print(array)